<p id="notice"><%= notice %></p>
<h1>Login</h1>
<div>
  <%= form_with(url: login_path, scope: :session, local: true) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, placeholder: "Name"%>
    <br>
    <div class="actions">
      <%= f.submit "Sign In" %>
      <br/><br/>SIGN UP HERE: <%= link_to 'New User', new_user_path %>
    </div>
  <%end%>
</div>
