<p id="notice"><%= notice %></p>
<h1>Users</h1>
<table>
    <thead>
        <tr>
            <th colspan="3">ID</th>
            <th colspan="3">Name</th>
        </tr>
    </thead>
    <tbody>
        <% @users.each do |user| %>
        <tr>
            <td><%= user.id %></td>
            <td><%= user.name %></td>
            <br>
        </tr>
        <% end %>
    </tbody>
</table>
<br>
<%= link_to 'New User', new_user_path %>